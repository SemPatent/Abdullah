<UserControl x:Class="Practicals_2014.Pages.GetPatentsResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="10">
        <DataGrid Name="dgUsers" AutoGenerateColumns="False" CanUserAddRows="false">
            <DataGrid.Columns>

                <DataGridTextColumn Header="S/N" Binding="{Binding Id}" />
                <DataGridTextColumn Header="Patent Document Number" Binding="{Binding DocNum}" />
                <DataGridTextColumn Header="English File Name" Binding="{Binding SaveNameEN}" />
                <DataGridTextColumn Header="Russian File Name" Binding="{Binding SaveNameRU}" />
                <DataGridTextColumn Header="Saved File Location" Binding="{Binding SaveFileLocation}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>

</UserControl>
